<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kaumobile">

    <application
        android:name=".application.Initialize"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KAUMobile">
        <receiver
            android:name=".ui.notifications.notification.AlarmReceiver"
            android:enabled="true"
            android:exported="true"></receiver>

        <activity android:name=".ui.Activity.TimeScheduleActivity" />
        <activity android:name=".ui.home.calendar.CalendarActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.sswolf.kausugang.Seong.NoteActivity" />
        <activity android:name=".ui.classNote.assign.AssignActivity" />
    </application>

</manifest>